{
    "name": "css-compact",
    "displayName": "css-compact",
    "description": "let's go back to original way to write our css",
    "version": "0.0.3",
    "publisher": "jsonchou",
    "Keywords":"css compact,format css,single line,single row",
    "author": {
        "name": "jsonchou",
        "url": "https://github.com/jsonchou"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jsonchou/cssCompact.git"
    },
    "bugs": {
        "url": "https://github.com/jsonchou/css-compact/issues",
        "email": "jsonchou@outlook.com"
    },
    "icon": "name.jpg",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onCommand:extension.cssCompact",
        "onLanguage:css"
    ],
    "main": "./extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "css compact",
            "properties": {
                "cssCompact.Author": {
                    "type": "string",
                    "default": "jsonchou",
                    "description": "author description"
                }
            }
        },
        "keybindings": [{
            "command": "extension.cssCompact",
            "key": "shift+alt+f",
            "when": "editorTextFocus && editorLangId == css"
        }],
        "commands": [{
            "command": "extension.cssCompact",
            "title": "cssCompact"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}